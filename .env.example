# Firebase Configuration
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Encryption Configuration
# Chave AES-256 em formato hexadecimal (64 caracteres = 256 bits)
# Gerar com: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
# IMPORTANTE: Esta chave deve ser armazenada nas GitHub Secrets em producao
VITE_ENCRYPTION_KEY=your_64_char_hex_key_here

# Logging Configuration
# Valores possíveis: debug | info | warn | error | none
# debug: Mostra todos os logs (desenvolvimento)
# info: Mostra info, warn e error (padrão desenvolvimento)
# warn: Mostra apenas warn e error
# error: Mostra apenas errors (padrão produção)
# none: Desabilita todos os logs
VITE_LOG_LEVEL=debug
